/*find factorial of a number */

import java.util.Scanner;
class factorial
{
	public static void main(String[] args)
	{
		long n,fact=1;
		System.out.println("Enter the any number:");
		Scanner r=new Scanner(System.in);
		n=r.nextInt();
		
		for(int i=1;i<=n;i++)
		{
			fact=fact*i;
		}
		System.out.println("Factorial of"+" "+n+" "+"is"+" "+fact);

	}
}