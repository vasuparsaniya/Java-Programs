/* Reverse Number program */
import java.util.Scanner;
class reverse
{
	public static void main(String[] args)
	{
		int n,r;
		System.out.println("Enter Any Number:");
		Scanner ref=new Scanner(System.in);
		n=ref.nextInt();
		
		while(n>0)
		{
			r=n%10;
			//System.out.print("Reverse order:"+r);
			System.out.print(r);
			n=n/10;
		}
	}
}

