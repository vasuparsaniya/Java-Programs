/* Assignment operator */

class Assignment
{
	public static void main(String[] args)
	{
		int a;
		a=10;  //= is assignment operator
		
		System.out.println(a);
		
		a+=10;  //compount assignment operator a=a+10;
		
		System.out.println(a);
		
		a-=10;  //compount assignment operator a=a-10;  
	}
}