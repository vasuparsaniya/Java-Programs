/* In simple Inheritance super class variable or method make private then saw what is out put?  */

class student  //super class
{
	 private int roll,marks;    //make variable as private
	String name; 
    
    private void input()     // make method as private
	{
     System.out.println("Enter roll,name & marks:");
	}
}

class king extends student    //sub class
{
	void disp()
	{
        roll=1;
		name="vasu";
		marks=89;
		
		System.out.println(roll+ " " +name+ " " +marks);
	}
	
	public static void main(String[] args)
	{
	    king r=new king();     //make object of sub class because sub class content all feature of super class
		r.input();
		r.disp();
	}
}	


/* --> private method can't access out side of them class
   --> In output saw error because in super class method or variable is private
*/